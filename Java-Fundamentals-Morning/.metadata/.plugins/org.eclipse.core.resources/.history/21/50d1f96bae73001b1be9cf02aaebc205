import java.util.*;

public class q2 {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		Map<Integer,String> map= new TreeMap<>();
		System.out.println("Enter number of elems: ");
		int n=sc.nextInt();
		for(int i=0;i<n;i++)
		{
			Integer k=sc.next();
			String v=sc.next();
			map.put(k, v);
		}
		System.out.println("Original Map: "+map.keySet());
		System.out.println("Reversed Map: "+ map.descendingKeySet());
	}
}
