import java.util.*;
public class q3 {
	
static boolean isPrime(int n) {
	boolean flag = false;
	for (int i = 2; i <= n / 2; i++) {
		if (n % i == 0) {
		        return true;
		}
	}
	return false;

}
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		
		for(int i=1;i<=40;i++) {
			if(isPrime(i)==true) {
				System.out.println(i+" is a prime no.");
				
			}
		}
	}
}
